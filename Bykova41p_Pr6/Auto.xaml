<Page x:Class="Bykova41p_Pr6.Auto"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Bykova41p_Pr6"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Auto">

    <Grid>
        <StackPanel>
            <GroupBox Header="Введите логин">
                <TextBox Name="TbLogin" Text="Логин"/>
            </GroupBox>
            <GroupBox Header="Введите пароль">
                <PasswordBox Name="PBPasw" Password="Пароль"/>
            </GroupBox>
            <Button Click="BTAuto_Click" Name="BTAuto">Авторизация</Button>
        </StackPanel> 
    </Grid>
</Page>
